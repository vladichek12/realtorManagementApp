<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>Phoneshop servlet api training</display-name>


    <display-name>Realtor web-app</display-name>

    <servlet>
        <servlet-name>loginPageServlet</servlet-name>
        <servlet-class>realtorManagementApp.web.LoginPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginPageServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>registrationServlet</servlet-name>
        <servlet-class>realtorManagementApp.web.RegistrationPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registrationServlet</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>adminPageServlet</servlet-name>
        <servlet-class>realtorManagementApp.web.AdminPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminPageServlet</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>

</web-app>
